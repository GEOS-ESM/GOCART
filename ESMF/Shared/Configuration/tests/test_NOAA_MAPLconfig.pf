                  call options_config9%read_options_config(test_config, 'test')
               case ('test10')
                  test_config = iter%value()
                  call options_config10%read_options_config(test_config, 'test')
               case ('test11')
                  test_config = iter%value()
                  call options_config11%read_options_config(test_config, 'test')
               case ('test12')
                  test_config = iter%value()
                  call options_config12%read_options_config(test_config, 'test')
               case ('test13')
                  test_config = iter%value()
                  call options_config13%read_options_config(test_config, 'test')
               case ('test14')
                  test_config = iter%value()
                  call options_config14%read_options_config(test_config, 'test')
               case ('test15')
                  test_config = iter%value()
                  call options_config15%read_options_config(test_config, 'test')
               case ('test16')
                  test_config = iter%value()
                  call options_config16%read_options_config(test_config, 'test')
               end select

               call iter%next()
            end do
         end select

         call main_iter%next()
      end do

      call file_stream%close()

      @assert_that(options_config1%state,         is(equal_to('test')))
      @assert_that(options_config1%reverseLevels, is(equal_to(0)))
