      main_iter = main_config%begin()
      do while(main_iter /= main_config%end())
         key => main_iter%key()

         select case(key)
         case ('test_options')
            config = main_iter%value()
            iter   = config%begin()
            do while(iter /= config%end())
               test_name => iter%key()

               select case(test_name)
               case ('test1')
                  test_config = iter%value()
                  call options_config1%read_options_config(test_config, 'test')
               case ('test2')
                  test_config = iter%value()
                  call options_config2%read_options_config(test_config, 'test')
               case ('test3')
                  test_config = iter%value()
                  call options_config3%read_options_config(test_config, 'test')
               case ('test4')
